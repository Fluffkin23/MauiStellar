<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiStellar2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiStellar2.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="MauiStellar2">

    <!-- Start Page Shell Content -->
    <ShellContent
        
        ContentTemplate="{DataTemplate views:startPage}"
        Route="startPage" />

    <!-- Pls Page Shell Content -->
    <ShellContent
        
        ContentTemplate="{DataTemplate views:pls}"
        Route="pls" />

    <!-- Pls Page Shell Content -->
    <ShellContent
    
    ContentTemplate="{DataTemplate views:HoroscopePage}"
    Route="HoroscopePage" />

</Shell>
